			{
				"ModifierName"	"modifier_exorcism"
				"Target" 		"CASTER"
			}

			"ApplyModifier"
			{
				"ModifierName"	"modifier_exorcism"
				"Target" 		"CASTER"
				"Duration"		"%AbilityDuration"
			}

			"FireSound"
